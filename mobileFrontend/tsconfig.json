{
   "extends": "expo/tsconfig.base",
   "compilerOptions": {
      "strict": true,
      "paths": {
         "@/*": ["./*"]
      }
   },
   "include": [
      "**/*.ts",
      "**/*.tsx",
      ".expo/types/**/*.ts",
      "expo-env.d.ts",
      "app/**/*.ts",
      "app/**/*.tsx",
      "app.d.ts",
      "app/*/*.ts",
      "app/*/*.tsx"
   ]
}
